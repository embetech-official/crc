{
    "version": 8,
    "cmakeMinimumRequired": {
        "major": 3,
        "minor": 20,
        "patch": 0
    },
    "include": [
        "cmake/embenet-toolchains/CMakePresets.json"
    ],
    "buildPresets": [
        {
            "name": "ut",
            "description": "Unit tests",
            "configurePreset": "native",
            "configuration": "Debug",
            "targets": [
                "crc_ut"
            ]
        },
        {
            "name": "documentation",
            "description": "Generate documentation",
            "configurePreset": "native",
            "targets": "crc_doc"
        },
        {
            "name": "install",
            "description": "Release target",
            "configurePreset": "native",
            "targets": "install"
        }
    ]
}